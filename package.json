{
  "name": "hicord",
  "version": "0.1.0",
  "description": "",
  "license": "MIT OR Apache-2.0",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run type-check && vite build",
    "serve": "vite preview",
    "type-check": "tsc -p tsconfig.json",
    "lint": "eslint --cache --ext .ts,.tsx .",
    "lint:fix": "eslint --cache --fix --ext .ts,.tsx .",
    "fmt": "prettier --write --cache .",
    "fmt:check": "prettier --check --cache .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ],
    "*.{json,yaml,yml}": [
      "prettier --write --cache"
    ]
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/eslint": "^8.21.3",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-c8": "^0.24.5",
    "@vitest/ui": "^0.24.5",
    "autoprefixer": "^10.4.14",
    "daisyui": "^2.51.5",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-solid": "^0.7.4",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "2.7.1",
    "solid-testing-library": "^0.3.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "3.1.6",
    "vite-plugin-checker": "0.5.0 - 0.5.1",
    "vite-plugin-solid": "^2.6.1",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "@picmo/popup-picker": "^5.8.1",
    "@solid-primitives/resize-observer": "^2.0.13",
    "luxon": "^3.3.0",
    "material-symbols": "^0.2.15",
    "picmo": "^5.8.1",
    "skyway-js": "^4.4.5",
    "solid-js": "^1.6.16"
  }
}
